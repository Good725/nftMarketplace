{
  "apps": [
    {
      "name": "app",
      "script": "server/index.js",
      "instances": 1,
      "autorestart": true,
      "wait_ready": true,
      "listen_timeout": 50000,
      "kill_timeout": 5000,
      "max_memory_restart": "500M",
      "watch": true,
      "time": true,
      "exec_mode": "cluster",
      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}
